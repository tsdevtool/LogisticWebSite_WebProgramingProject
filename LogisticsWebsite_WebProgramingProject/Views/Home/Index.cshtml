<!-- TRACK SECTION START HERE -->
<section class="track-shipment-section w-100 float-left padding-top padding-bottom">
    <div class="container">
        <div class="tracking-outer-con" data-aos="fade-up" data-aos-duration="700">
            <div class="tracking-left-con">
                <div class="generic-title">
                    <span>Shippment Tracking</span>
                    <h2>
                        Track or Calculate Your
                        Shipment With Us
                    </h2>
                </div>
                <p class="mb-0">
                    Nemo enim ipsam voluptatem quia voluptas sit aspernatur aut odit aut
                    fugit, sed quia consequuntur magni dolores eos qui ratione voluptatem
                    sequi nesciunt nerue porro ruisuam.
                </p>
            </div>
            <div class="tracking-right-con">
                <div class="tracking-code">
                    <h3>
                        Track Your Shipment
                        Now
                    </h3>
                    <input type="text" placeholder="Enter Your Shipment Code" required>
                    <select name="service" id="service">
                        <option value="service">Select Your Service</option>
                        <option value="warehouse">Warehouse</option>
                        <option value="aircargo">Air Cargo</option>
                        <option value="shipment">Shipment</option>
                    </select>
                    <button type="submit"><a class="dropdown-item" asp-controller="BillOfLading" asp-action="Details">Track Now</a></button>
                </div>
            </div>
        </div>
    </div>
</section>
